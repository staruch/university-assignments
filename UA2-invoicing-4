//trieda pre vytvorenie zakaznika pre fakturaciu
public class Customer
{
    private String name;
    private String address;
    
    public Customer(String i_name, String i_address)
    {
        name = i_name;
        address = i_address;
    }

    
    public void setAddress(String i_address)
    {
        address = i_address;
    }
    
    public String getName()
    {
        return name;
    }
    
    public String getAddress()
    {
        return address;
    }
    
    public String toString() //metoda vypisania textu pri volani funkcie, kde sa ocakava string
    {
        return " " + name + ", " + address;
    }
    
}
